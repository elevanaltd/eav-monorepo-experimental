{
  "buildCommand": "cd ../.. && pnpm turbo run build --filter=eav-scripts-web",
  "installCommand": "cd ../.. && corepack enable && pnpm install",
  "outputDirectory": "dist",
  "framework": "vite",
  "functions": {
    "api/webhook-smartsuite.ts": {
      "maxDuration": 10
    },
    "api/sync-manual.ts": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}