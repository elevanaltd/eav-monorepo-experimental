{
  "name": "@elevanaltd/shared",
  "version": "0.5.0",
  "description": "Unified shared library for EAV Operations Suite - utilities, database client, UI components, and types",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./client": {
      "import": "./dist/lib/client/index.js",
      "require": "./dist/lib/client/index.cjs",
      "types": "./dist/lib/client/index.d.ts"
    },
    "./types": {
      "import": "./dist/lib/types/index.js",
      "require": "./dist/lib/types/index.cjs",
      "types": "./dist/lib/types/index.d.ts"
    },
    "./auth": {
      "import": "./dist/lib/auth/index.js",
      "require": "./dist/lib/auth/index.cjs",
      "types": "./dist/lib/auth/index.d.ts"
    },
    "./rls": {
      "import": "./dist/lib/rls/index.js",
      "require": "./dist/lib/rls/index.cjs",
      "types": "./dist/lib/rls/index.d.ts"
    },
    "./navigation": {
      "import": "./dist/lib/navigation/index.js",
      "require": "./dist/lib/navigation/index.cjs",
      "types": "./dist/lib/navigation/index.d.ts"
    },
    "./components": {
      "import": "./dist/components/index.js",
      "require": "./dist/components/index.cjs",
      "types": "./dist/components/index.d.ts"
    },
    "./dist/index.css": "./dist/index.css"
  },
  "scripts": {
    "build": "tsup && npm run build:css",
    "build:css": "cat src/**/*.css > dist/index.css 2>/dev/null || true",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "validate": "npm run lint && npm run typecheck && npm run test"
  },
  "keywords": [
    "eav",
    "supabase",
    "shared-library",
    "ui",
    "react",
    "components"
  ],
  "author": "",
  "license": "ISC",
  "peerDependencies": {
    "@supabase/supabase-js": "^2.75.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.90.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.38.0",
    "@supabase/supabase-js": "^2.76.1",
    "@testing-library/dom": "^10.0.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^20.19.23",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitejs/plugin-react": "^4.3.3",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.38.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "globals": "^16.4.0",
    "jsdom": "^27.0.1",
    "postcss": "^8.4.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3",
    "vite": "^7.1.11",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
